{
  "name": "enterprise-platform",
  "version": "1.0.0",
  "description": "Advanced Enterprise Management Platform with Real-time Analytics, AI-powered Insights, and Comprehensive Business Intelligence",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm run dev",
    "build": "npm run client:build && npm run server:build",
    "client:build": "cd client && npm run build",
    "server:build": "babel server -d dist --copy-files",
    "start": "node dist/index.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:e2e": "cypress run",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "docker:build": "docker build -t enterprise-platform .",
    "docker:run": "docker-compose up -d",
    "deploy": "npm run build && pm2 start ecosystem.config.js",
    "backup": "node scripts/backup-database.js",
    "restore": "node scripts/restore-database.js",
    "generate:docs": "apidoc -i server/routes -o docs/api",
    "analyze": "npm run client:analyze && npm run server:analyze",
    "client:analyze": "cd client && npm run analyze",
    "server:analyze": "madge --circular server/",
    "security:audit": "npm audit && snyk test",
    "performance:test": "artillery run performance/load-test.yml"
  },
